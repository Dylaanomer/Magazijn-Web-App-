@model  MagazijnPartij

<div class="modal fade" id="addMagazijnPartij">
    
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="addMagazijnPartijLabel">  Bewerk Partijserie</h4>
                <button type="button"  class="close" data-dismiss="modal">
                    <span>x</span>
                </button>
                
                <div class="modal-body">


                    <form>
                        <div class="form-group">
                            <label asp-for="PHerk">@Model.PHerk</label>
                            
                        </div>
                        <div class="form-group">
                            <label asp-for="Naam">@Model.Naam</label>
                            
                        </div>
                        <div class="form-group">
                            <label asp-for="VpNaam">@Model.VpNaam</label>
                            
                        </div>
                        <div class="form-group">
                            <label asp-for="MagazijnId">@Model.MagazijnId</label>
                            
                        </div>
                        <div class="form-group">
                            <label asp-for="StadiumId">@Model.StadiumId</label>
                        </div>
                        <div class="form-group">
                            <label asp-for="Pvan">@Model.Pvan</label>
                        </div>
                        <div class="form-group">
                            <label asp-for="Ptot">@Model.Ptot</label>
                            
                        </div>


                    </form>
                
                    <div class="modal-footer">
                  
                        <button type="button"  class="btn btn-primary" data-dismiss="modal"> Close  </button>

                    </div>
                     


                
                
                </div>
            </div>
        </div>


