@model ALPHA_DGS.Models.Magazijn

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Edit</h1>

<h4>Magazijn</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="ParentMloId" class="control-label"></label>
                <input asp-for="ParentMloId" class="form-control" />
                <span asp-validation-for="ParentMloId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LokatieType" class="control-label"></label>
                <input asp-for="LokatieType" class="form-control" />
                <span asp-validation-for="LokatieType" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Naam" class="control-label"></label>
                <input asp-for="Naam" class="form-control" />
                <span asp-validation-for="Naam" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Sequence" class="control-label"></label>
                <input asp-for="Sequence" class="form-control" />
                <span asp-validation-for="Sequence" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Indicator" class="control-label"></label>
                <input asp-for="Indicator" class="form-control" />
                <span asp-validation-for="Indicator" class="text-danger"></span>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="Bezet" /> @Html.DisplayNameFor(model => model.Bezet)
                </label>
            </div>
            <div class="form-group">
                <label asp-for="MaxAanFust" class="control-label"></label>
                <input asp-for="MaxAanFust" class="form-control" />
                <span asp-validation-for="MaxAanFust" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
